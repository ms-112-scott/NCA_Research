    "import torch\n",
    "\n",
    "# 檢查 GPU 是否可用\n",
    "print(\"CUDA Available:\", torch.cuda.is_available())\n",
    "\n",
    "# 顯示 GPU 數量\n",
    "print(\"GPU Count:\", torch.cuda.device_count())\n",
    "\n",
    "# 顯示第一張 GPU 的名稱\n",
    "if torch.cuda.is_available():\n",
    "    print(\"GPU Name:\", torch.cuda.get_device_name(0))\n"
   ]
  }
